import pandas as pd
import numpy as np
import pandas
import openpyxl
from openpyxl import load_workbook

# inport date. Find a better format. ddmmyy is wanted for date added to saved file at end.
from datetime import date
from openpyxl import load_workbook

# Open Daily Excel?

# Today's Date

today = date.today()
# Open Fuesbox CSV
fusebox_csv = input("Enter Fusebox location here: ")
df = pd.read_csv(fusebox_csv)

fusebox_row = df.shape[0]
print("Fusebox Rows = " + str(fusebox_row))
print("\n")

wb = load_workbook(filename="/Users/DCB/Google Drive/Python/zBLANK - CHS.xlsx")
sheet_ranges = wb['Adult SSS']

# Not sure. I think it's just making the excel available to add to
sheet = wb.active
# index_max = count_row

# while count_row

payment_type = df["PAYMENT_TYPE"]
# print(descrip[1])
f = fusebox_row

while f >= 1:
    f = f - 1
    if f == -1:
        break
    if f == 0:
        if payment_type[0] == "AX":
            amx = df.iat[0, 14]
            sheet["D39"] = amx
            continue
        else:
            if payment_type[0] == "MC":
                mastercard = df.iat[0, 14]
                continue
            else:
                if payment_type[0] == "VI":
                    visa = df.iat[0, 14]
                    continue
                else:
                    if payment_type[0] == "DI":
                        discover = df.iat[0, 14]
                        continue
                    else:
                        print("Error")
                        pass
    if f == 1:
        if payment_type[1] == "AX":
            amx = df.iat[1, 14]
            sheet["D39"] = amx
            continue
        else:
            if payment_type[1] == "MC":
                mastercard = df.iat[1, 14]
                continue
            else:
                if payment_type[1] == "VI":
                    visa = df.iat[1, 14]
                    continue
                else:
                    if payment_type[1] == "DI":
                        discover = df.iat[1, 14]
                        continue
                    else:
                        print("Error")
                        pass
    if f == 2:
        if payment_type[2] == "AX":
            amx = df.iat[2, 14]
            sheet["D39"] = amx
            continue
        else:
            if payment_type[2] == "MC":
                mastercard = df.iat[2, 14]
                continue
            else:
                if payment_type[2] == "VI":
                    visa = df.iat[2, 14]
                    continue
                else:
                    if payment_type[2] == "DI":
                        discover = df.iat[2, 14]
                        continue
                    else:
                        print("Error")
                        pass
    if f == 3:
        if payment_type[3] == "AX":
            amx = df.iat[3, 14]
            sheet["D39"] = amx
            continue
        else:
            if payment_type[3] == "MC":
                mastercard = df.iat[3, 14]
                continue
            else:
                if payment_type[3] == "VI":
                    visa = df.iat[3, 14]
                    continue
                else:
                    if payment_type[3] == "DI":
                        discover = df.iat[3, 14]
                        continue
                    else:
                        print("Error")
                        pass

try:
    sheet["D38"] = mastercard + visa + discover
except:
    try:
        sheet["D38"] = mastercard + discover
    except:
        try:
            sheet["D38"] = mastercard + visa
        except:
            try:
                sheet["D38"] = visa + discover
            except:
                try:
                    sheet["D38"] = mastercard
                except:
                    try:
                        sheet["D38"] = visa
                    except:
                        try:
                            sheet["D38"] = discover
                        except:
                            print("No Credit Cards")

# inport date. Find a better format. ddmmyy is wanted for date added to saved file at end.
from datetime import date
from openpyxl import load_workbook

summary_csv = input("Enter Summary Location: ")
df = pd.read_csv(summary_csv)

# print(mydata01)
# print(lines)
# print(df["PC_DESCRIP,C,35"])

count_row = df.shape[0]
print("Rows = " + str(count_row))
print("\n")

# index_max = count_row

# while count_row

descrip = df["pc_descrip"]
# print(descrip[1])
i = count_row

while i >= 1:
    i = i - 1
    if i == -1:
        break
    if i == 0:
        if descrip[0] == "60-RETAIL - 43400-620":
            retail = df.iat[0, 3]
            sheet["D21"] = retail
            print(descrip[0] + " added")
            continue
        else:
            if descrip[0] == "50-RENTAL - 43100-600":
                rental = df.iat[0, 3]
                sheet["D20"] = rental
                print(descrip[0] + " added")
                continue
            else:
                if descrip[0] == "10-LIFT TICKETS - 40000-300":
                    tickets = df.iat[0, 3]
                    sheet["D6"] = tickets
                    print(descrip[0] + " added")
                    continue
                else:
                    if descrip[0] == "30-SS GRP SKI - 42000-533":
                        ski = df.iat[0, 3]
                        sheet["D12"] = ski
                        print(descrip[0] + " added")
                        continue
                    else:
                        if descrip[0] == "40-CHILDCARE - 42300-546":
                            childcare = df.iat[0, 3]
                            sheet["D17"] = childcare
                            print(descrip[0] + " added")
                            continue
                        else:
                            if descrip[0] == "41-CC MEALS - 44000-716":
                                meals = df.iat[0, 3]
                                sheet["D18"] = meals
                                print(descrip[0] + " added")
                                continue
                            else:
                                if descrip[0] == "40-CC RENTAL - 43100-545":
                                    ccrental = df.iat[0, 3]
                                    sheet["D15"] = ccrental
                                    print(descrip[0] + " added")
                                    continue
                                else:
                                    if descrip[0] == "40-CC SKI - 42000-543":
                                        ccski = df.iat[0, 3]
                                        sheet["D16"] = ccski
                                        print(descrip[0] + " added")
                                        continue
                                    else:
                                        if descrip[0] == "40-CC BOARD - 42005-544":
                                            ccsbd = df.iat[0, 3]
                                            sheet["D14"] = ccsbd
                                            print(descrip[0] + " added")
                                            continue
                                        else:
                                            if descrip[0] == "30-SS GRP BOARD - 42005-535":
                                                sbd = df.iat[0, 3]
                                                sheet["D11"] = sbd
                                                print(descrip[0] + " added")
                                                continue
                                            else:
                                                if descrip[0] == "GRT":
                                                    grt = df.iat[0, 3]
                                                    sheet["D27"] = grt
                                                    print(descrip[0] + " added")
                                                    continue
                                                else:
                                                    if descrip[0] == "ACCOUNT PAYMENTS & CREDITS":
                                                        payments = df.iat[0, 3]
                                                        sheet["D24"] = payments
                                                        print(descrip[0] + " added")
                                                        continue
                                                    else:
                                                        if descrip[0] == "ACCOUNT CHARGES":
                                                            charges = df.iat[0, 3]
                                                            sheet["D23"] = charges
                                                            print(descrip[0] + " added")
                                                            continue
                                                        else:
                                                            if descrip[0] == "AMERICAN EXPRESS":
                                                                amx_siri_neg = df.iat[0, 3]
                                                                amx_siri_pos = -amx_siri_neg
                                                                print(descrip[0] + " added")
                                                                if amx == amx_siri_pos:
                                                                    continue
                                                                else:
                                                                    print("!!! AMX Fusebox does NOT match Sirius !!!")
                                                                    print("AMX is off by: " + str(amx - amx_siri_pos))
                                                                    input("Enter any key to continue:")
                                                                    continue
                                                            else:
                                                                if descrip[0] == "DISCOVER":
                                                                    di_siri_neg = df.iat[0, 3]
                                                                    di_siri_pos = -di_siri_neg
                                                                    print(descrip[0] + " added")
                                                                    if discover == di_siri_pos:
                                                                        continue
                                                                    else:
                                                                        print(
                                                                            "!!! DI Fusebox does NOT match Sirius !!!")
                                                                        print("Discover is off by: " + str(
                                                                            discover - di_siri_pos))
                                                                        input("Enter any key to continue:")
                                                                        continue
                                                                else:
                                                                    if descrip[0] == "MASTERCARD":
                                                                        mc_siri_neg = df.iat[0, 3]
                                                                        mc_siri_pos = -mc_siri_neg
                                                                        print(descrip[0] + " added")
                                                                        if mastercard == mc_siri_pos:
                                                                            continue
                                                                        else:
                                                                            print(
                                                                                "!!! MC Fusebox does NOT match Sirius !!!")
                                                                            print("Mastercard is off by: " + str(
                                                                                mastercard - mc_siri_pos))
                                                                            input("Enter any key to continue:")
                                                                            continue
                                                                    else:
                                                                        if descrip[0] == "VISA":
                                                                            vi_siri_neg = df.iat[0, 3]
                                                                            vi_siri_pos = -vi_siri_neg
                                                                            print(descrip[0] + " added")
                                                                            if visa == vi_siri_pos:
                                                                                continue
                                                                            else:
                                                                                print(
                                                                                    "!!! VI Fusebox does NOT match Sirius !!!")
                                                                                print("Visa is off by: " + str(
                                                                                    visa - vi_siri_pos))
                                                                                input("Enter any key to continue:")
                                                                                continue
                                                                        else:
                                                                            if descrip[0] == "96-REVREC DEFER - 27175-000":
                                                                                revrec_defer_siri = df.iat[0, 3]
                                                                                sheet["D22"] = revrec_defer_siri
                                                                                print(descrip[0] + " added")
                                                                                continue
                                                                            else:
                                                                                if descrip[
                                                                                    0] == "CHECK REQUEST":
                                                                                    check_request_siri = df.iat[0, 3]
                                                                                    sheet["D41"] = check_request_siri
                                                                                    print(descrip[0] + " added")
                                                                                    continue
                                                                                else:
                                                                                    if descrip[
                                                                                        0] == "SNOWSPORT SQUARE":
                                                                                        square_siri = df.iat[
                                                                                            0, 3]
                                                                                        sheet[
                                                                                            "D45"] = square_siri
                                                                                        print(descrip[0] + " added")
                                                                                        continue
                                                                                    else:
                                                                                        if descrip[
                                                                                            0] == "CHECK":
                                                                                            check_siri = input(
                                                                                                "How much $ in checks?: ")
                                                                                            sheet[
                                                                                                "D33"] = check_siri
                                                                                            print(
                                                                                                descrip[0] + " added")
                                                                                            continue
                                                                                        else:
                                                                                            if descrip[
                                                                                                0] == "40-NT CC BOARD - 42006-544":
                                                                                                nt_ccsbd_siri = df.iat[
                                                                                                    0, 3]
                                                                                                sheet[
                                                                                                    "J11"] = nt_ccsbd_siri
                                                                                                print(
                                                                                                    descrip[
                                                                                                        0] + " added")
                                                                                                continue
                                                                                            else:
                                                                                                if descrip[
                                                                                                    0] == "40-NT CC RENTAL - 43101-545":
                                                                                                    nt_ccrental_siri = \
                                                                                                        df.iat[
                                                                                                            0, 3]
                                                                                                    sheet[
                                                                                                        "J13"] = nt_ccrental_siri
                                                                                                    print(
                                                                                                        descrip[
                                                                                                            0] + " added")
                                                                                                    continue
                                                                                                else:
                                                                                                    if descrip[
                                                                                                        0] == "41-NT CC MEALS - 44001-716":
                                                                                                        nt_ccmeals_siri = \
                                                                                                            df.iat[
                                                                                                                0, 3]
                                                                                                        sheet[
                                                                                                            "J14"] = nt_ccmeals_siri
                                                                                                        print(
                                                                                                            descrip[
                                                                                                                0] + " added")
                                                                                                        continue
                                                                                                    else:
                                                                                                        if descrip[
                                                                                                            0] == "10-NT LIFT - 40001-300":
                                                                                                            nt_lift_siri = \
                                                                                                                df.iat[
                                                                                                                    0, 3]
                                                                                                            sheet[
                                                                                                                "J7"] = nt_lift_siri
                                                                                                            print(
                                                                                                                descrip[
                                                                                                                    0] + " added")
                                                                                                            continue
                                                                                                        else:
                                                                                                            if \
                                                                                                                    descrip[
                                                                                                                        0] == "40-NT CC SKI - 42001-543":
                                                                                                                nt_ccski_siri = \
                                                                                                                    df.iat[
                                                                                                                        0, 3]
                                                                                                                sheet[
                                                                                                                    "J9"] = nt_ccski_siri
                                                                                                                print(
                                                                                                                    descrip[
                                                                                                                        0] + " added")
                                                                                                                continue
                                                                                                            else:
                                                                                                                print(
                                                                                                                    "Error.0")
