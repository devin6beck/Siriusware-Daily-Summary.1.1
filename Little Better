                                                                                                                df.iat[
                                                                                                                    21, 3]
                                                                                                            sheet[
                                                                                                                "J7"] = nt_lift_siri
                                                                                                            print(
                                                                                                                descrip[
                                                                                                                    21] + " added")
                                                                                                            continue
                                                                                                        else:
                                                                                                            if \
                                                                                                            descrip[
                                                                                                                21] == "40-NT CC SKI - 42001-543":
